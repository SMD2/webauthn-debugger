{
    "name": "Webauthn Debugger",
    "description": "Debug Webauthn API",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["injection.js"]
      }
    ],
    "permissions":[
      "tabs",
      "storage"
    ],
    "background": {
      "page": "background.html"
    },
    "web_accessible_resources": [
      "wrapper.js",
      "cbor.js"
    ]
  }

  	

  	